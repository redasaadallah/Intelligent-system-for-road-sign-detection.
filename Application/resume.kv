<Resume>:
    name:"resume"
    md_bg_color: 255,255,255,1
    Image:
        source: 'logo/cover6.jpg'  # Replace with your image path
        allow_stretch: True
        keep_ratio: False  # Set to False to cover the entire window
    ScrollView:
        size:self.size
        do_scroll_x:True
        do_scroll_y:True
        BoxLayout:
            orientation:"vertical"
            padding:(0,1300,0,550)
            size_hint_y:None
            height:self.minimum_height
            width:self.minimum_width
            Screen:
                size_hint_y:None
                height:300
                width:400
                
                Image:
                    source:"logo/8.png"
                    size_hint:0.5,.5
                    pos_hint: {'center_x': 0.25,'center_y': 4.85}
                MDBoxLayout:
                    size_hint: 0.00000000001,.4
                    radius:100
                    md_bg_color:255,153/255,53/255,1
                    pos_hint: {'center_x': 0.43,'center_y': 4.85}
                    canvas.after:
                        Color:
                            rgba: 255, 153/255, 51/255, 1
                        Line:
                            width: 3
                            ellipse: self.x, self.y, self.width, self.height
                MDLabel:
                    text: "L'IA sur la Route,\n Sécurité Optimisée."
                    halign:'center'
                    pos_hint:{'center_x':0.7,'center_y':4.85}
                    bold:True  
                Image:
                    source:"logo/writing.png"
                    size_hint:0.4,1
                    pos_hint: {'center_x': 0.5,'center_y': 4.3}  
                MDLabel:
                    text: "Vos notifications!"
                    halign:'center'
                    pos_hint:{'center_x':0.5,'center_y':3.9}
                    theme_text_color: "Custom"
                    text_color: "orange"
                    font_style:'Headline'
                    role:"small"
                    bold:True
                 
                        
                                
                MDBoxLayout:
                    cols:1
                    pos_hint:{'center_x':0.5,'center_y':1.3}
                    size_hint:0.9,5
                    
                    
                    MDScreen:
                        id:cn
                        
                        Image:
                            source: 'logo/cover1.jpeg'  
                            allow_stretch: True
                            keep_ratio: False
                        MDLabel:
                            bold:True
                            font_size:14
                            text: "[color=#FF8C00]Bienvenue dans l'application mobile.[/color] "
                            pos_hint: {'center_x': 0.5,'center_y': 0.97}
                            halign:"center"
                            font_style:'Title'
                            role:"large"
                            markup:True
                        MDLabel:
                            id:cher
                            text: 'M. SAADALLAH Reda'
                            pos_hint: {'center_x': 0.5,'center_y': 0.91}
                        MDLabel:
                            id:msg
                            pos_hint: {'center_x': 0.5,'center_y': 0.8}
                            text: ""
                        
                        
                        MDLabel:
                            id:npn
                            markup:True
                            pos_hint: {'center_x': 0.5,'center_y': 0.56}
                            text: ""

                        
                        MDLabel:
                            pos_hint: {'center_x': 0.5,'center_y': 0.41}
                            text: "Cordialement," 
                    
                        MDLabel:
                            id:nom
                            pos_hint: {'center_x': 0.5,'center_y': 0.39}
                            text:' A bientot M. SAADALLAH Reda'

                    
                        
                        Image:
                            source:'logo/8.png'
                            pos_hint: {'center_x': 0.5,'center_y': 0.2}
                            size_hint:1,1
                            halign:"center"
               
                Image:
                    source:"logo/7.png"
                    size_hint:.5,0.5
                    halign:"center"
                    pos_hint: {'center_x': 0.5,'center_y':-1.5}
                MDLabel:
                    text: "Tous droits réservés 2025"
                    halign:'center'
                    pos_hint:{'center_x':0.5,'center_y':-1.65}
                    theme_text_color: "Custom"
                    text_color: "blue"
                    font_style:'Label'
                    role:'medium'
    
               

    MDTopAppBar:
        type: "small"
        size_hint_x: 1
        pos_hint: {"center_x": .5, "center_y": 0.96}
        
        MDTopAppBarLeadingButtonContainer:
            md_bg_color:255,0,0
            MDActionTopAppBarButton:
                style:'filled'
                icon: "arrow-left"
                on_release:
                    app.root.current="home"
                    app.root.transition.direction ='right'
                    #app.root.transition.duration=0
                
                
        MDTopAppBarTitle:
            text: " M. REDA SAADALLAH"
            font_style:'Title'
            role:'medium'
          
            
        MDTopAppBarTrailingButtonContainer:

            
            
            
            MDActionTopAppBarButton:
                icon: "account-box-outline"
            MDActionTopAppBarButton:
                style:'filled'
                icon: "menu"
                on_release:nav_drawer.set_state('open')
                
    
            

    MDNavigationDrawer:
        id: nav_drawer
        MDScreen:
            size_hint:1,1
            Image:
                source: 'logo/cover1.jpeg'  # Replace with your image path
                allow_stretch: True
                keep_ratio: False  # Set to False to cover the entire window
            
            Image:
                source:"logo/8.png"
                size_hint:.5,.5
                pos_hint: {'center_x': 0.5,'center_y': 0.8}
            MDButton:
                style: "elevated"
                pos_hint: {"center_x": .5, "center_y": .5}
                on_release:
                    app.deconnecter()
                    nav_drawer.set_state('close')
                MDButtonIcon:
                    icon: "logout"
                MDButtonText:
                    text: "Déconnecter"
                    bold:True
            MDButton:
                style: "text"
                pos_hint: {"center_x": .5, "center_y": .19}
                MDButtonIcon:
                    icon: "google"
                MDButtonText:
                    text: "Google"
                    bold:True
            MDButton:
                style: "text"
                pos_hint: {"center_x": .5, "center_y": .12}
                MDButtonIcon:
                    icon: "facebook"
                MDButtonText:
                    text: "Facebook"
                    bold:True
            MDButton:
                style: "text"
                pos_hint: {"center_x": .5, "center_y": .05}
                MDButtonIcon:
                    icon: "instagram"
                MDButtonText:
                    text: "Instagram"
                    bold:True